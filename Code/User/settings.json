{
  "workbench.startupEditor": "none",
  "keyboard.dispatch": "keyCode",
  "editor.fontFamily": "JetBrainsMono NF SemiBold",
  "terminal.integrated.fontFamily": "JetBrainsMono NF SemiBold",
  "workbench.colorTheme": "Monokai Palenight ",
  "window.menuBarVisibility": "toggle",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "colorize.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg",
    "rasi",
    "javascript"
  ],
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",

  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["l", "w"],
      "after": ["$"]
    }
  ],
  "workbench.iconTheme": "vscode-icons",
  "vscord.status.details.text.editing": "In {workspace}",
  "vscord.status.details.text.viewing": "In {workspace}",
  "vscord.status.state.text.editing": "Working on {file_name}{file_extension}",
  "editor.fontLigatures": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "explorer.confirmDelete": false,
  "workbench.colorCustomizations": {
    "terminal.background":"#252838",
    "terminal.foreground":"#f0ecec",
    "terminal.ansiBlack":"#1D2021",
    "terminal.ansiBrightBlack":"#665C54",
    "terminal.ansiBrightBlue":"#78dce8",
    "terminal.ansiBrightCyan":"#fc9867",
    "terminal.ansiBrightGreen":"#a9dc76",
    "terminal.ansiBrightMagenta":"#ab9df2",
    "terminal.ansiBrightRed":"#ff6188",
    "terminal.ansiBrightWhite":"#FDF4C1",
    "terminal.ansiBrightYellow":"#ffd866",
    "terminal.ansiBlue":"#78dce8",
    "terminal.ansiCyan":"#fc9867",
    "terminal.ansiGreen":"#a9dc76",
    "terminal.ansiMagenta":"#ab9df2",
    "terminal.ansiRed":"#ff6188",
    "terminal.ansiWhite":"#A89984",
    "terminal.ansiYellow":"#ffd866"
  },
}
