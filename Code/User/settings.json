{
  "workbench.startupEditor": "none",
  "keyboard.dispatch": "keyCode",
  "editor.fontFamily": "JetBrainsMono NF SemiBold",
  "terminal.integrated.fontFamily": "JetBrainsMono NF SemiBold",
  "workbench.colorTheme": "Monokai Palenight ",
  "window.menuBarVisibility": "toggle",
  "window.dialogStyle": "custom",
  "window.titleBarStyle": "custom",
  "colorize.languages": [
    "css",
    "sass",
    "scss",
    "less",
    "postcss",
    "sss",
    "stylus",
    "xml",
    "svg",
    "rasi",
    "javascript"
  ],
  "files.autoSave": "onFocusChange",
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",

  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["l", "w"],
      "after": ["$"]
    }
  ],
  "workbench.iconTheme": "vscode-icons",
  "vscord.status.details.text.editing": "In {workspace}",
  "vscord.status.details.text.viewing": "In {workspace}",
  "vscord.status.state.text.editing": "Working on {file_name}{file_extension}",
  "editor.fontLigatures": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.smoothScrolling": true,
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "explorer.confirmDelete": false
}
